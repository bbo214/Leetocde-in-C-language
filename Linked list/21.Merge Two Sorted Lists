struct ListNode* mergeTwoLists(struct ListNode* l1, struct ListNode* l2){
    if(l1==NULL)
        return l2;
    if(l2==NULL)
        return l1;
    if(l2==NULL&&l1==NULL)
        return false;
    struct ListNode* p;
    p = 11;
    while(l1&&l2) {
        if(l1->val>=l2->val){
        l1->next =l2;     
        11=l1->next;
        l2=l2->next;
        }
        else if(l1->val<l2->val){
            l1->next = l2;
            p = p->next;
            l1 = l1->next;
        }
    while(l1){
        p->next =l1;
        l1 = l1->next;
    }
    while(l2){
        p->next = l2;
        l2 =l2->next;  
    }
    }
    return p->next;
}
